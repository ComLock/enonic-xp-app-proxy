<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<task>
	<description>Persist url</description>
	<config>
		<input name="absoluteUrl" type="Textline">
			<label>Url to persist</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
		<input name="persistLookupTable" type="CheckBox">
			<label>Persist lookup table</label>
			<default>checked</default>
		</input>
		<input name="serviceUrl" type="Textline">
			<label>Service url</label>
			<occurrences minimum="1" maximum="1"/>
		</input>
	</config>
</task>
